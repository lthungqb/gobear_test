          "required": [
            "InsurerID",
            "InsurerName",
            "InsurerSmallLogo",
            "PlanID",
            "PlanName",
            "Premium",
            "OriginalPremium",
            "DiscountValue",
            "DiscountUnit",
            "DiscountUntill",
            "HasQuotes",
            "PromotionID",
            "PromotionTitle",
            "PromotionContent",
            "TopCoverages",
            "CheckoutOptions",
            "HasPaidOptions",
            "SelectedPaidOptionsCount",
            "SelectedPaidOptions",
            "DocumentLink",
            "TotalPaidOptionsCount"
          ]
        }
