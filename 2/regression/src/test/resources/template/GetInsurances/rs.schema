                  "required": [
                    "PaymentModel",
                    "SupplierID",
                    "SupplierName",
                    "SupplierLogo",
                    "IsBroker",
                    "Link",
                    "Premium",
                    "SKUCode",
                    "CheckoutNotes"
                  ]
                }
              ]
            },
            "HasPaidOptions": {
              "type": "boolean"
            },
            "SelectedPaidOptionsCount": {
              "type": "integer"
            },
            "SelectedPaidOptions": {
              "type": "array",
              "items": [
                {
                  "type": "array",
                  "items": [
                    {
                      "type": "object",
                      "properties": {
                        "ID": {
                          "type": "string"
                        },
                        "SelectedValue": {
                          "type": "null"
                        }
                      },
                      "required": [
                        "ID",
                        "SelectedValue"
                      ]
                    }
                  ]
                }
              ]
            },
            "DocumentLink": {
              "type": "null"
            },
            "TotalPaidOptionsCount": {
              "type": "integer"
            }
